@model IEnumerable<Student>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Course</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">  
</head>
<body>
    <table>
        <thead>
            <tr>
                <td>Name</td>
                <td>Email</td>
                <td>Phone</td>
                <td>Confirm</td>
            </tr>

        </thead>
        <tbody>

            @foreach (var Student in Model)
            {
            <tr>
                <td>@Student.name</td>
                <td>@Student.email</td>
                <td>@Student.phone</td>
                <td>@Student.confirm</td>
            </tr>




            }

        </tbody>
    </table>
    
    

</body>
</html>